target/
*.jar
!target/orchestrator-*.jar

# IDE files
.idea/
.vscode/
*.iml
*.ipr
*.iws
.project
.classpath
.settings/

# OS files
.DS_Store
Thumbs.db
*.log

# Git files
.git/
.gitignore

# Environment files
.env
.env.local
.env.*.local
.env.docker

# Documentation
README.md
HELP.md
*.md

# Docker files (not needed in container)
docker-compose.yml
docker-compose.*.yml
Dockerfile
.dockerignore

# Scripts
*.sh
*.bat

